    <td><%= link_to 'Show', bark_parse %></td>
    <td><%= link_to 'Edit', edit_bark_parse_path(bark_parse) %></td>
    <td><%= link_to 'Destroy', bark_parse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
