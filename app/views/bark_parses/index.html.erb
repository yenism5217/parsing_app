<% @bark_parses.each do |bark_parse| %>
  <tr>
    <td><%= bark_parse.bark_id %></td>
    <td><%= bark_parse.x_choice %></td>
    <td><%= bark_parse.x_hyper0 %></td>
    <td><%= bark_parse.x_hyper1 %></td>
